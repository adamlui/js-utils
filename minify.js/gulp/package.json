    },
    {
      "type": "ko-fi",
      "url": "https://ko-fi.com/adamlui"
    }
  ],
  "main": "dist/gulp-minify.min.js",
  "files": [
    "docs/"
  ],
  "directories": {
    "lib": "./dist",
    "doc": "./docs"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "bash utils/build.sh",
    "bump:patch": "bash utils/bump.sh patch",
    "bump:minor": "bash utils/bump.sh minor",
    "bump:major": "bash utils/bump.sh major",
    "publish:patch": "bash utils/bump.sh patch --publish",
    "publish:minor": "bash utils/bump.sh minor --publish",
    "publish:major": "bash utils/bump.sh major --publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adamlui/minify.js.git"
  },
  "keywords": [
    "javascript",
    "utilities",
    "js-utils",
    "minifier",
    "api",
    "js",
    "utils",
    "gulpplugin",
    "gulpfriendly"
  ],
  "bugs": {
    "url": "https://support.minify-js.org"
  },
  "dependencies": {
    "@adamlui/minify.js": "^1.8.5"
  }
