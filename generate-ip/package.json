  "main": "generate-ip.js",
  "files": [ "dist", "docs" ],
  "bin": {
    "generateip": "generate-ip.js",
    "generate-ip": "generate-ip.js"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "minify-js generate-ip.js dist && cp generate-ip.js ./dist 2> nul || copy generate-ip.js dist",
